<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.moodtracker.dao.BoardDao">
	
	<select id="memberList" parameterType="String" resultType="hashMap">
		SELECT * 
		FROM EMPLOYEE
		WHERE 1=1
		AND	id = #{id}
	</select>

</mapper>